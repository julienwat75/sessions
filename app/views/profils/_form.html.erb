<%= form_for(@profils) do |f| %>
  <ul>
  <% @profils.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
  </ul>
  <p>
    <%= f.label :nom %><br />
    <%= f.text_field :nom %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :body %>
    <p>
  <%= f.label :tag_list %><br />
  <%= f.text_field :tag_list %>
</p>

  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
